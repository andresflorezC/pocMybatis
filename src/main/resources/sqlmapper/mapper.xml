<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.poc.mybatis.mapper.Mapper">

    <resultMap id="userMap" type="com.sb.poc.cursorsmybatis.dto.DBUser">
        <id property="userId" column="USER_ID" />
        <result property="username" column="USERNAME" />
        <result property="createdBy" column="CREATED_BY" />
        <result property="createdDate" column="CREATED_DATE" />
    </resultMap>

    <resultMap id="creationTimesMap" type="com.sb.poc.cursorsmybatis.dto.CreationTimes">
        <id property="userId" column="USER_ID" />
        <result property="diffHours" column="DIFF_HOURS" />
    </resultMap>

</mapper>
